<?xml version="1.0" encoding="UTF-8" ?>
<class name="TrainElectricEngine" inherits="TrainEngine" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
    <brief_description>
        Electric engine handler class. Used to check/set it's parameters
    </brief_description>
    <description>
        Class handling electric engines
    </description>
    <tutorials>
    </tutorials>
    <members>
        <member name="power/accumulator/recharge_source" type="int" setter="set_accumulator_recharge_source"
                getter="get_accumulator_recharge_source" default="0">
            Power source for recharging the accumulator
        </member>
        <member name="power/current_collector/csw" type="float" setter="set_csw" getter="get_csw" default="0.0">
            Working width of the collector/pantograph slide, in meters
        </member>
        <member name="power/current_collector/max_collector_lifting" type="float" setter="set_max_collector_lifting"
                getter="get_max_collector_lifting" default="0.0">
            Maximum collector lifting, in meters.
        </member>
        <member name="power/current_collector/max_current" type="float" setter="set_max_current"
                getter="get_max_current" default="0.0">
            Max current on the current collector (pantograph)
        </member>
        <member name="power/current_collector/max_pantograph_tank_pressure" type="float"
                setter="set_max_pantograph_tank_pressure" getter="get_max_pantograph_tank_pressure" default="0.0">
            Maximum pressure in the Pantograph Tank
        </member>
        <member name="power/current_collector/max_voltage" type="float" setter="set_max_voltage"
                getter="get_max_voltage" default="0.0">
            Max voltage on the current collector (pantograph)
        </member>
        <member name="power/current_collector/min_collector_lifting" type="float" setter="set_min_collector_lifting"
                getter="get_min_collector_lifting" default="0.0">
            Minimal collector lifting from the folded position, in meters.
        </member>
        <member name="power/current_collector/min_main_switch_voltage" type="float" setter="set_min_main_switch_voltage"
                getter="get_min_main_switch_voltage" default="0.0">
            The minimum closing voltage of the main switch, in volts.
            By default, it is 0.5*max_voltage
        </member>
        <member name="power/current_collector/min_pantograph_tank_pressure" type="float"
                setter="set_min_pantograph_tank_pressure" getter="get_min_pantograph_tank_pressure" default="0.0">
            Minimum pressure in the Pantograph Tank (PT) to raise the pantograph, in MPa
        </member>
        <member name="power/current_collector/number_of_collectors" type="int" setter="set_number_of_collectors"
                getter="get_number_of_collectors" default="0">
            NUmber of collectors on the train
        </member>
        <member name="power/current_collector/overvoltage_relay" type="bool" setter="set_overvoltage_relay"
                getter="get_overvoltage_relay" default="false">
        </member>
        <member name="power/current_collector/required_main_switch_voltage" type="float"
                setter="set_required_main_switch_voltage" getter="get_required_main_switch_voltage" default="0.0">
            Required voltage to switch the main switch on.
            By default, it is 0.6*max_voltage
        </member>
        <member name="power/power_cable/source" type="int" setter="set_power_cable_power_source"
                getter="get_power_cable_power_source" default="0">
            Energy source for the power cable
        </member>
        <member name="power/power_cable/steam_pressure" type="float" setter="set_power_cable_steam_pressure"
                getter="get_power_cable_steam_pressure" default="0.0">
            Steam pressure in the power cable. Applies only when the energy source for the power cable is steam
        </member>
        <member name="power/source" type="int" setter="set_engine_power_source" getter="get_engine_power_source"
                default="0">
            Energy source for the engine
        </member>
        <member name="power/transducer/input_voltage" type="float" setter="set_transducer_input_voltage"
                getter="get_transducer_input_voltage" default="0.0">
            Input voltage for the transducer
        </member>
        <member name="switches/compressor" type="bool" setter="set_compressor_switch_pressed"
                getter="get_compressor_switch_pressed" default="false">
            Is the compressor on
        </member>
        <member name="switches/converter" type="bool" setter="set_converter_switch_pressed"
                getter="get_converter_switch_pressed" default="false">
            Is the converter on
        </member>
    </members>
</class>
